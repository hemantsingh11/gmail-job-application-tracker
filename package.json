{
  "name": "jobapp",
  "version": "1.0.0",
  "description": "Local profile webapp with Express + Azure Cosmos DB",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "npm-run-all --parallel dev:server client:dev",
    "dev:server": "tsx src/server.ts",
    "build": "npm-run-all build:server build:client",
    "build:server": "tsc",
    "build:client": "npm run client:build",
    "client:dev": "node scripts/wait-for.js 3000 && vite",
    "client:build": "vite build",
    "client:preview": "vite preview"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@azure/cosmos": "^4.7.0",
    "@azure/identity": "^4.13.0",
    "@azure/keyvault-secrets": "^4.10.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "googleapis": "^166.0.0",
    "html-to-text": "^9.0.5",
    "node-cron": "^4.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.9.0",
    "@types/node-cron": "^3.0.11",
    "@types/html-to-text": "^9.0.4",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "npm-run-all": "^4.1.5",
    "tsx": "^4.19.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.8"
  }
}
